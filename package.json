{
  "name": "manny",
  "version": "2.3.15",
  "description": "npm package for importing @mannynotfound into your website.",
  "main": "./lib/manny.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.config.js --mode development --open",
    "generate:library": "node generate-library.js",
    "build:package": "webpack --config webpack.build.config.js --progress --mode production",
    "build": "npm run generate:library && npm run build:package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mannynotfound/manny.git"
  },
  "author": "@mannynotfound",
  "license": "WTFPL",
  "dependencies": {
    "three": "0.137.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "^4.5.4",
    "html-webpack-plugin": "3.2.0",
    "webpack": "4.7.0",
    "webpack-cli": "2.0.0",
    "webpack-dev-server": "3.1.4"
  },
  "keywords": [
    "manny",
    "three.js"
  ],
  "homepage": "http://art404.com"
}
